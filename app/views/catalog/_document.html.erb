<% # container for a single doc -%>
<article class="document <%= render_document_class document %> document-position-<%= document_counter%> " data-document-counter="<%= document_counter %>" itemscope itemtype="<%= document.itemtype %>">
  <%= Blacklight::IndexDocumentPartialsRenderer.new(presenter: index_presenter(document),
                                                   partials: blacklight_config.view_config(document_index_view_type).partials,
                                                   view: self,
                                                   view_type: document_index_view_type,
                                                   view_cache: view_cache).render(document_counter: document_counter) %>
</article>
